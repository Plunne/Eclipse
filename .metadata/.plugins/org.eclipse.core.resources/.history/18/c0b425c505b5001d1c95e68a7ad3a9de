#include <stdio.h>
#include <stdlib.h>

int main(void) {

	char var[5] = {0x10, 0x20, 0x00, 0x40, 0x55};

	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */

	var[2] = 0x30;
	var[0] = 0x30;

	printf("Var third %d", var[2]); /* prints !!!Hello World!!! */

	return EXIT_SUCCESS;
}
